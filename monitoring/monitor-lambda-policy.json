{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublishReporting",
      "Effect": "Allow",
      "Action": ["sns:Publish"],
      "Resource": [ 
         "arn:aws:sns::534691387561:MarchonErrors"
       ]
    },    
    {
      "Sid": "ReadLogs",
      "Effect": "Allow",
      "Action": [
                 "logs:GetLogEvents", 
                 "logs:FilterLogEvents"
                 ],
      "Resource": [ 
        "arn:aws:logs::534691387561:log-group:/aws/lambda/marchon-events-geojson:*"
      ]
    }
  ]
}